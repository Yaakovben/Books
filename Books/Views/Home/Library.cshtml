@model IEnumerable<Books.Models.Library>

@{
    ViewData["Title"] = "Library";
}

<h1>הוספת מדף לספרייה</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genre)
            </td>
            <td>
                  
                    @Html.ActionLink("הוסף מדף", "CreateShelf2", new {id=item.Id}) |
                   
               
            </td>
        </tr>
}
    </tbody>
</table>
